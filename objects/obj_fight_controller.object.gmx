            <string>///Turn handler and gladiators

turn_handler = instance_create(-1,-1, obj_turner)
global.selected = noone

for (i = 0; i &lt; ds_list_size(global.gladiators); i++) {

    do {
        xx = room_width/2 - 64 + 32*i
        yy = room_height/2 + 32*irandom_range(-5,5)
